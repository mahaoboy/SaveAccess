<html>
  <head>
    <title>$i18n.getText("PageAccess.section.name")</title>
    <meta name="decorator" content="atl.admin">
    <meta name="section" content="SaveAccessSection/SaveAccessWebItem"/>
    <meta name="tab" content="SaveAccessWebItem"/>
    $webResourceManager.requireResource("com.atlassian.auiplugin:ajs")
  </head>
  <body>
    <table>
    	<tr><td>$i18n.getText("PageAccess.section.pageName")</td>
    		<td>$i18n.getText("PageAccess.section.username")</td>
    		<td>$i18n.getText("PageAccess.section.accessCount")</td>
    		<td>$i18n.getText("PageAccess.section.userGroup")</td>
    		<td>$i18n.getText("PageAccess.section.accessDate")</td>
    	</tr>
    	 #foreach($item in $asResult)
          <tr>
          	<td><a href="$item.get("pageUrl")" target="_blank">$item.get("pageTitle")</a></td>
          	<td>$item.get("userName")</td>
          	<td>$item.get("accessCount")</td>
          	#set($userGroupsWithHtml = $item.get("userGroups"))
          	<td>$userGroupsWithHtml</td>
          	#set($uniqueKeyWithHtml = $accessDate.get($item.get("uniqueKey")))
          	<td>$uniqueKeyWithHtml</td>
          </tr>
		#end
    </table>
  </body>
</html>